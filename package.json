{
    "name": "Pattern-Library",
    "version": "0.0.1",
    "description": "A study project of create a pattern library based on React.js",
    "main": "src/app.js",
    "scripts": {
      "test": "jest",
      "start": "webpack-dev-server --devtool eval --progress --hot --colors --content-base src",
      "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
      "validate": "npm ls",
      "lint": "node_modules/.bin/esw webpack.config.* src tools",
      "lint:watch": "npm run lint -- --watch",
      "clean-dist": "npm run remove-dist && mkdir dist",
      "remove-dist": "node_modules/.bin/rimraf ./dist",
      "build:html": "babel-node tools/buildHtml.js",
      "prebuild": "npm-run-all clean-dist build:html",
      "build": "babel-node tools/build.js",
      "postbuild": "babel-node tools/publicServer.js"
    },
    "engines": {
      "node": "6",
      "npm": "3"
